nota=10
print(input(nota<=10))
=True


nota=6
faltas=4
print(input(nota<=6 and faltas<=3))
=False

convidados=3
fumante=False
print(input(convidados>4 or fumante==True))
=False

dia="qua"
print(input(dia=="sab" or dia=="dom"))
=False

feriado=True
print(input(not(feriado==False)))
=True

dia="ter"
feriado=False
print(input(dia=="seg" or not(feriado==False)))
=False